
/**
 * Ejercicio 1) Remover elementos duplicados.
 */
#include <unistd.h>
#include <stdio.h>

#define SIZE 14

int main(int argc, char **argv)
{
    int sample[SIZE] = {0, 9, 8, 2, 2, 2, 5, 9, 5, 0, 3, 2, 3, 7};
    int i;

    for (i = 0; i < SIZE; i++) {
        printf("%d\n", sample[i]);
        // TODO ...
    }

    return 0;
}

/**
 * Ejercicio 1) Remover elementos duplicados.
 */
#include <unistd.h>
#include <stdio.h>

#define SIZE 14

int main(int argc, char **argv)
{
    int sample[SIZE] = {0, 9, 8, 2, 2, 2, 5, 9, 5, 0, 3, 2, 3, 7};
    int i;
    int nuevo[SIZE];
    int aux;
    int j;
    int a;
    for (i = 0; i < SIZE; i++) {
        aux = sample[i];
        a = 0;
        //printf("sws");
        for (j=0; j < SIZE; j++) {
                if (aux == nuevo[j]){
                  a=1;
        //      printf("lalalalala");                   
                }
        }
        if (a == 0) {
          nuevo[i] = aux;
          printf("%d\n", nuevo[i]);
        }        // TODO ...
    }

    return 0;
}

